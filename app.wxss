@import "/static/css/iconfont.wxss";

page {
    width: 100%;
    height: 100%;
    overflow: hidden;
    background-color: #fff;
    box-sizing: border-box;
    position: relative;
}

view {
    box-sizing: border-box;
}

scroll-view {
    box-sizing: border-box;
}

input {
    box-sizing: border-box;
}

navigator {
    box-sizing: border-box;
}

swiper {
    box-sizing: border-box;
}

image {
    box-sizing: border-box;
}

textarea {
    box-sizing: border-box;
}
