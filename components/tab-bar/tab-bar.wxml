<!--components/tab-bar/tab-bar.wxml-->
<view class="tab-bar-component" style="height:{{style.height}};">
    <slot></slot>
    <view wx:for="{{tabBarData.items}}" wx:key="{{index}}" style="width:calc(100% / {{tabBarData.items.length}});" class="tab-bar-item" wx:if="{{childType !== 'slot'}}">
        <view class="tab-bar-type-col">
            <image class="tab-bar-col-img"></image>
            <text class="tab-bar-col-text">{{item.label}}</text>
        </view>
    </view>
</view>